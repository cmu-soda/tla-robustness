--------------------------- MODULE SimpleSysFixed ---------------------------

EXTENDS Naturals

VARIABLES i, j

vars == <<i,j>>

I ==
    /\ i < 3
    /\ i' = i + 1
    /\ j' = j

J ==
    /\ j < 3
    /\ j' = j + 1
    /\ i' = i


Init ==
    \/ /\ i = 0
       /\ j = 0

I_Guarded == I /\ ~(i = 0 /\ j = 1) /\ ~(i = 1 /\ j = 3) /\ ~(i = 0 /\ j = 0) /\ ~(i = 2 /\ j = 2) /\ ~(i = 2 /\ j = 3) /\ ~(i = 2 /\ j = 0) /\ ~(i = 2 /\ j = 1) /\ ~(i = 1 /\ j = 0) /\ ~(i = 1 /\ j = 1) /\ ~(i = 0 /\ j = 3) /\ ~(i = 0 /\ j = 2) /\ ~(i = 1 /\ j = 2)
J_Guarded == J /\ ~(i = 3 /\ j = 2) /\ ~(i = 3 /\ j = 1) /\ ~(i = 3 /\ j = 0) /\ ~(i = 2 /\ j = 2) /\ ~(i = 2 /\ j = 0) /\ ~(i = 2 /\ j = 1) /\ ~(i = 1 /\ j = 0) /\ ~(i = 1 /\ j = 1) /\ ~(i = 1 /\ j = 2)
Next ==
  \/ I_Guarded
  \/ J_Guarded

Spec == Init /\ [][Next]_vars


TypeOK ==
    /\ i \in Nat
    /\ j \in Nat

Safety ==
    /\ i = 0
    /\ j <= 3


=============================================================================
